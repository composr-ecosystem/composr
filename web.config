		<defaultDocument>
			<files>
				<clear />
				<add value="index.html" />
				<add value="index.php" />
			</files>
		</defaultDocument>

		<!-- LEGACY: .dat files are now .bin, but old ones still may be around -->
		<staticContent>
			<remove fileExtension=".dat" />
			<mimeMap fileExtension=".dat" mimeType="application/octet-stream" />
		</staticContent>

		<httpProtocol>
			<customHeaders>
				<add name="X-Content-Type-Options" value="nosniff" />
			</customHeaders>
		</httpProtocol>

		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="2147483648" />
				<!--2GB; also see maxRequestLength-->

				<hiddenSegments>
					<add segment="banned_access.log" />
					<add segment="errorlog.php" />
					<add segment="permission_checks.log" />
					<add segment="profiling*.log" />
					<add segment="ecommerce.log" />
					<add segment="resource_fs.log" />
					<add segment="big_query_screens.log" />
					<add segment="queries.log" />
					<add segment="fields.xml" />
					<add segment="breadcrumbs.xml" />
					<add segment="cron.log" />
					<add segment="health_check.log" />
					<add segment="debug_fs.log" />
					<add segment="mail_integration.log" />
					<add segment="tasks.log" />

					<!-- For non-bundled addons, but important -->
					<add segment="hybridauth.xml" />
				</hiddenSegments>
			</requestFiltering>
		</security>

		<!-- Note that the path needs updating if we're in a sub-directory.
			<httpErrors errorMode="DetailedLocalOnly" existingResponse="Replace">
				<remove statusCode="404" subStatusCode="-1" />
				<error statusCode="404" path="/index.php?page=404" responseMode="ExecuteURL" />
			</httpErrors>
		-->
	</system.webServer>

	<system.web>
		<httpRuntime executionTimeout="100000" maxRequestLength="2147483647" />
		<sessionState timeout="30" />
	</system.web>
</configuration>
